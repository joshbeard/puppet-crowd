{
  "name": "prikkeldraad-crowd",
  "version": "1.0.5",
  "author": "prikkeldraad",
  "summary": "Atlassian Crowd Puppet Module",
  "license": "Apache-2.0",
  "source": "https://github.com/prikkeldraad/puppet-crowd",
  "project_page": "https://github.com/prikkeldraad/puppet-crowd",
